<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_bottom_bar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mehadihossain.mapfragmentadd.Activities.BottomBarActivity">

    <com.roughike.bottombar.BottomBar
        android:id="@+id/bottomBar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        app:bb_behavior="underNavbar"
        app:bb_tabXmlResource="@xml/navigation_bar_menu" >
    </com.roughike.bottombar.BottomBar>

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:layout_margin="10dp"
        app:fabSize="normal"
        app:srcCompat="@drawable/locb"
        android:id="@+id/floatingActionButton"
        android:layout_above="@+id/bottomBar"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="match_parent"
        android:layout_height="76dp"
        android:clickable="true"
        app:fabSize="normal"
        android:layout_alignParentRight="true"
        android:layout_margin="10dp"
        android:onClick="doPrint"
        app:srcCompat="@drawable/printer"
        android:id="@+id/printActionButton" />

    <FrameLayout
        android:id="@+id/container1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

    </FrameLayout>
</RelativeLayout>
